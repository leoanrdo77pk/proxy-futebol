{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [ 
    // Redirecionamentos personalizados
    { "src": "/redirect/1", "dest": "https://links3.futemais.net/" },
    { "src": "/redirect/2", "dest": "https://outrolink.com/" },
    
    // Rota padrão que chama o arquivo index.js para o conteúdo
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
